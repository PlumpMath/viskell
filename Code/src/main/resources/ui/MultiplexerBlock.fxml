<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.TilePane?>
<fx:root type="nl.utwente.viskell.ui.components.MultiplexerBlock" xmlns:fx="http://javafx.com/fxml/">
    <BorderPane>
        <top>
            <TilePane fx:id="inputSpace" maxHeight="0" alignment="CENTER" />
        </top>
        <center>
            <BorderPane fx:id="block" styleClass="function, block" minHeight="75" maxHeight="75" minWidth="150">
            </BorderPane>
        </center>
        <bottom>
            <Pane fx:id="outputSpace" maxHeight="0" />
        </bottom>
    </BorderPane>
</fx:root>
