<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.HBox?>
<?import nl.utwente.viskell.ui.components.RGBBlock?>
<fx:root type="nl.utwente.viskell.ui.components.RGBBlock" xmlns:fx="http://javafx.com/fxml/">
    <BorderPane styleClass="rgb, block" minHeight="75" maxHeight="75" minWidth="150" maxWidth="150">
        <top>
            <HBox fx:id="inputSpace" spacing="50" alignment="CENTER" />
        </top>
        <center>
            <Pane fx:id="well"/>
        </center>
        <bottom>
            <HBox fx:id="outputSpace" maxHeight="0"/>
        </bottom>
    </BorderPane>
</fx:root>
